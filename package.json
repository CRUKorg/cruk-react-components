{
  "name": "@cruk/cruk-react-components",
  "version": "0.1.5",
  "description": "React components implementing CRUK and SU2C designs",
  "license": "UNLICENSED",
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/cjs/src/components/index.js",
  "module": "lib/es/src/components/index.js",
  "jsnext:main": "lib/es/index.js",
  "types": "lib/components/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib"
  ],
  "engines": {
    "node": "^14.0.0",
    "npm": "^7"
  },
  "scripts": {
    "dev": "NODE_ENV=development npm i && docker-compose up server",
    "start": "webpack server --config webpack.config.docs.js --mode development",
    "clean-lib": "rm -rf ./lib",
    "build-lib": "npm run clean-lib && webpack --config webpack.config.lib.js --mode production",
    "build-docs": "webpack --config webpack.config.docs.js --mode production",
    "preanalyze": "npm run build-lib",
    "analyze": "source-map-explorer lib/index.js",
    "cypress-headless": "docker-compose stop cypress && docker-compose run cypress $ARGS",
    "cypress-headless:update": "docker-compose stop cypress && docker-compose run cypress -e updateSnapshots=true $ARGS",
    "cypress": "npx cypress open-ct",
    "jest-ci": "NODE_ENV=test jest --watchAll=false",
    "jest": "NODE_ENV=test jest --watch",
    "test": "npm run jest-ci && npm run cypress-headless",
    "test:update": "NODE_ENV=test npm run cypress-headless:update",
    "percy": "percy storybook:start",
    "clean": "rm -rf node_modules/",
    "release": "node release.js",
    "rollup:build-lib": "npm run clean-lib && rollup --config",
    "prettier": "prettier --write src/**/*.{ts,tsx}",
    "prettier:test": "prettier --check src/**/*.{ts,tsx}",
    "prettier:test-staged": "npx pretty-quick --pattern 'src/**/*.{js,ts,tsx}' 'cypress/{integration,plugins,support}/**/*.{js,ts,tsx}' --staged",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "eslint:test": "eslint src",
    "eslint:test-staged": "npx lint-staged",
    "prepare": "husky install",
    "precommit": "npm run prettier:test-staged && npm run eslint:test-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CRUKorg/cruk-react-components.git"
  },
  "bugs": {
    "url": "https://github.com/CRUKorg/cruk-react-components/issues"
  },
  "homepage": "https://github.com/CRUKorg/cruk-react-components#readme",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.9.0",
    "@cypress/react": "^5.10.0",
    "@cypress/webpack-dev-server": "1.6.0",
    "@cypress/webpack-preprocessor": "^5.9.1",
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@percy/cli": "^1.0.0-beta.65",
    "@percy/storybook": "^4.0.1",
    "@reach/router": "^1.3.4",
    "@react-theming/storybook-addon": "^1.1.1",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@storybook/addon-a11y": "6.3.8",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/builder-webpack5": "^6.3.8",
    "@storybook/manager-webpack5": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@types/assert": "^1.5.1",
    "@types/cypress-image-snapshot": "^3.1.6",
    "@types/jest": "^26.0.9",
    "@types/mdx-js__react": "^1.5.1",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "axe-core": "^4.2.1",
    "babel-loader": "8.2.1",
    "babel-plugin-styled-components": "^1.8.0",
    "cypress": "8.4.1",
    "cypress-axe": "0.13.0",
    "cypress-image-snapshot": "^4.0.1",
    "cypress-plugin-tab": "^1.0.5",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.1.2",
    "prettier": "^1.19.1",
    "pretty-quick": "^3.1.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-focus-lock": "^2.5.0",
    "react-live": "^2.2.3",
    "rollup": "^2.7.2",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "source-map-explorer": "^2.2.2",
    "styled-components": "^5.1.1",
    "ts-loader": "9.2.1",
    "typescript": "^4.4.3",
    "webpack": "^5.53.0",
    "webpack-cli": "4.8.0",
    "webpack-dev-server": "^3.11.2"
  },
  "peerDependencies": {
    "react": "^16.12.0 || ^17.0.1 ",
    "react-dom": "^16.12.0 || ^17.0.1 ",
    "styled-components": "^5.1.1"
  },
  "dependencies": {}
}
