{
  "name": "@crukorg/cruk-react-components",
  "version": "0.1.0",
  "description": "React components implementing CRUK and SU2C design tokens",
  "main": "lib/index.js",
  "types": "lib/components/index.d.ts",
  "files": ["lib"],
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.docs.js --open --hot --mode development --historyApiFallback",
    "build-lib": "webpack --config webpack.config.lib.js --mode production",
    "build-docs": "webpack --config webpack.config.docs.js --mode production",
    "preanalyze": "npm run build-lib",
    "analyze": "source-map-explorer lib/index.js",
    "test-dev": "$(npm bin)/cypress open",
    "prettier": "$(npm bin)/prettier --write 'components/**/*.tsx'",
    "test": "$(npm bin)/cypress run",
    "clean": "rm -rf node_modules/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build-lib && git add ./lib"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CRUKorg/cruk-react-components.git"
  },
  "bugs": {
    "url": "https://github.com/CRUKorg/cruk-react-components/issues"
  },
  "homepage": "https://github.com/CRUKorg/cruk-react-components#readme",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@mdx-js/loader": "^1.5.5",
    "@mdx-js/mdx": "^0.20.3",
    "@reach/router": "^1.2.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^4.4.2",
    "axe-core": "^3.4.2",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.6",
    "cypress": "4.0.1",
    "cypress-axe": "^0.5.3",
    "cypress-image-snapshot": "^3.1.1",
    "cypress-plugin-tab": "^1.0.5",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.1",
    "prettier": "1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-focus-lock": "^2.2.1",
    "react-live": "^2.0.1",
    "source-map-explorer": "^2.2.2",
    "styled-components": "^5.0.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.0.0"
  }
}
