{
  "name": "@crukorg/cruk-react-components",
  "version": "0.1.0-alpha.23",
  "description": "React components implementing CRUK and SU2C design tokens",
  "main": "lib/index.js",
  "types": "lib/components/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.docs.js --open --hot --mode development --historyApiFallback",
    "clean-lib": "rm -rf ./lib",
    "build-lib": "npm run clean-lib && webpack --config webpack.config.lib.js --mode production",
    "build-docs": "webpack --config webpack.config.docs.js --mode production",
    "preanalyze": "npm run build-lib",
    "analyze": "source-map-explorer lib/index.js",
    "prettier": "$(npm bin)/prettier --write 'components/**/*.tsx'",
    "cypress-ci": "docker-compose stop cypress && docker-compose run cypress",
    "cypress": "$(npm bin)/cypress open",
    "jest-ci": "jest --watchAll=false",
    "jest": "jest --watch",
    "test": "NODE_ENV=test npm run jest-ci && npm run cypress-ci",
    "clean": "rm -rf node_modules/",
    "release": "node release.js",
    "dev": "NODE_ENV=development npm i && docker-compose up server"
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CRUKorg/cruk-react-components.git"
  },
  "bugs": {
    "url": "https://github.com/CRUKorg/cruk-react-components/issues"
  },
  "homepage": "https://github.com/CRUKorg/cruk-react-components#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.9.0",
    "@mdx-js/loader": "^1.5.5",
    "@mdx-js/mdx": "^0.20.3",
    "@reach/router": "^1.2.1",
    "@types/jest": "^25.1.3",
    "@types/mdx-js__react": "^1.5.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^4.4.2",
    "axe-core": "^3.4.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.8.0",
    "cypress": "^4.4.1",
    "cypress-axe": "^0.8.1",
    "cypress-image-snapshot": "^3.1.1",
    "cypress-plugin-tab": "^1.0.5",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.1",
    "jest": "^25.4.0",
    "prettier": "1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-focus-lock": "^2.2.1",
    "react-live": "^2.0.1",
    "source-map-explorer": "^2.2.2",
    "styled-components": "^5.0.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.0.0"
  }
}
