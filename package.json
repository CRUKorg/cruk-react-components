{
  "name": "@cruk/cruk-react-components",
  "version": "5.0.8",
  "description": "React components implementing CRUK, RFL, SU2C & Bowelbabe designs",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "module": "lib/src/components/index.js",
  "jsnext:main": "lib/index.js",
  "types": "lib/components/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "NODE_ENV=development npm i && npm run start",
    "start": "webpack server --config webpack.config.docs.js --mode development",
    "clean-lib": "rm -rf ./lib",
    "preanalyze": "npm run build-lib",
    "analyze": "source-map-explorer lib/index.js",
    "cypress-headless": "npx cypress run --component",
    "cy:open": "npx cypress open --component",
    "node-test": " node --import tsx --test-reporter=spec --test 'src/utils/__tests__/testHelpers.test.ts'",
    "node-test-watch": "node --import tsx --test-reporter=spec --watch --test 'src/utils/__tests__/testHelpers.test.ts'",
    "test": "npm run node-test && npm run cypress-headless",
    "clean": "rm -rf node_modules/",
    "release": "release-it",
    "rollup:build-lib": "npm run clean-lib && rollup --config",
    "prettier": "prettier --write '**/*.{ts,tsx}'",
    "prettier:test": "prettier --check src/**/*.{ts,tsx}",
    "prettier:debug": "prettier --debug-check src/**/*.{ts,tsx}",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "eslint:test": "eslint src",
    "eslint:fix": "eslint src --fix",
    "chromatic": "chromatic --exit-zero-on-changes"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CRUKorg/cruk-react-components.git"
  },
  "bugs": {
    "url": "https://github.com/CRUKorg/cruk-react-components/issues"
  },
  "homepage": "https://github.com/CRUKorg/cruk-react-components#readme",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.9.0",
    "@chromatic-com/storybook": "^3.2.4",
    "@cruk/eslint-config": "^4",
    "@cypress/webpack-dev-server": "3.5.3",
    "@cypress/webpack-preprocessor": "^6.0.0",
    "@mdx-js/loader": "^2.2.1",
    "@mdx-js/mdx": "^2.2.1",
    "@mdx-js/react": "^2.2.1",
    "@rollup/plugin-commonjs": "^26.0.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-a11y": "^8.5.3",
    "@storybook/addon-docs": "^8.5.3",
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/addon-interactions": "^8.5.3",
    "@storybook/addon-links": "^8.5.3",
    "@storybook/addon-webpack5-compiler-swc": "^2.0.0",
    "@storybook/react": "^8.5.3",
    "@storybook/react-webpack5": "^8.5.3",
    "@storybook/test": "^8.5.3",
    "@types/assert": "^1.5.1",
    "@types/node": "^20",
    "@types/react": "^18.0.1",
    "@types/react-dom": "^18.0.1",
    "axe-core": "^4.8.2",
    "babel-loader": "8.3.0",
    "babel-plugin-styled-components": "^2.0.2",
    "chromatic": "^6.21.0",
    "cypress": "^13.3.2",
    "cypress-axe": "1.5.0",
    "cypress-plugin-tab": "^1.0.5",
    "eslint": "^9",
    "eslint-config-prettier": "^10",
    "eslint-plugin-prettier": "^5",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-storybook": "^0.11.2",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.3.2",
    "prettier": "^3",
    "prop-types": "^15.8.1",
    "react": "^18.0.1",
    "react-dom": "^18.0.1",
    "rollup": "^4.22.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "source-map-explorer": "^2.2.2",
    "storybook": "^8.5.3",
    "ts-loader": "9.5.1",
    "tsx": "^4.16.2",
    "typescript": "^5.0.2",
    "typescript-eslint": "^8.20.0",
    "webpack": "^5.53.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "^4.7.3"
  },
  "peerDependencies": {
    "react": "^18.0.1",
    "react-dom": "^18.0.1",
    "styled-components": "^6.1.11"
  },
  "dependencies": {
    "@fortawesome/fontawesome-common-types": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "react-focus-lock": "^2.5.2",
    "react-intersection-observer": "^9.1.0",
    "react-is": "^18.2.0",
    "release-it": "^17.10.0",
    "styled-components": "^6.1.11"
  }
}
